---
permalink: .htaccess
---
RewriteEngine on
RewriteCond %{REQUEST_URI} "^\/?[0-9]{4}\/[0-9]{2}\/[A-Za-z0-9\-\_]{3}.*$"
RewriteRule "^([0-9]{4})\/([0-9]{2})\/.*$" "/$1/$2/" [R=301,L]

RewriteRule "^database\/tracks?/(.*?)\/?$" "http://db.fullofwishes.co.uk/tracks/$1/" [R=301,L]

RewriteRule "^database\/(galaxie-500|luna|damon-and-naomi|dean-and-britta|dean-wareham)-shows(.+?)\/?$" "http://db.fullofwishes.co.uk/$1/shows/" [R=301,L]
RewriteRule "^database\/(galaxie-500|luna|damon-and-naomi|dean-and-britta|dean-wareham)(.+?)\/?$" "http://db.fullofwishes.co.uk/$1/" [R=301,L]
RewriteRule "^database\/show\/[0-9]{4}-[0-9]{2}-[0-9]{2}-(galaxie-500|luna|damon-and-naomi|dean-and-britta|dean-wareham)(.+?)\/?$" "http://db.fullofwishes.co.uk/$1/shows/" [R=301,L]
RewriteRule "^database\/gigography\/(galaxie-500|luna|damon-and-naomi|dean-and-britta|dean-wareham)(.+?)\/?$" "http://db.fullofwishes.co.uk/$1/shows/" [R=301,L]
RewriteRule "^database\/(.+)?$" "http://db.fullofwishes.co.uk/" [R=301,L]
RewriteRule "^category\/bands\/(galaxie-500|luna|damon-and-naomi|dean-and-britta|dean-wareham|after-lunapage/2/)(.+)$" "/category/$1" [R=301,L]
RewriteRule "^category\/bands\/dean-britta(.+)$" "/category/dean-and-britta" [R=301,L]
RewriteRule "^category\/(.*?)\/(.+)$" "/category/" [R=301,L]
RewriteRule "^wp/wp-content/uploads(2)?/(.*)$" "http://media.fullofwishes.co.uk/ahfow/uploads$1/$2" [R=301,L]
RewriteRule "^2016/05/30/the-first-ahfow-podcast-how-does-luna-make-you-feel/(.*)$" "/2016/06/01/the-first-ahfow-podcast-how-does-luna-make-you-feel/" [R=301,L]
RewriteRule "^2010/06/15/audio-a-fleet-of-tugboats/(.*)$" "/articles/a-fleet-of-tugboats/$1" [R=301,L]
RewriteRule "^([0-9]{4})/([0-9]{2})/[0-9]{2}?(/|/index\.html)?$" "/$1/$2/" [R=301,L]