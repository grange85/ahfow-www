---
layout: none
---
[
  {% for post in site.posts %}
    {
      "title"    : "{% if post.series-title %}{{post.series-title | escape}}: {% endif %}{{ post.title | escape }}",
      "category" : "{{ post.categories | join: ', '  }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for page in site.galaxie-500-releases %}
   {
     {% if page.title != nil %}
        "title"    : "{{ page.title | escape }}",
